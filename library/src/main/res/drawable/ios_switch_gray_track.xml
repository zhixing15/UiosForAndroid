<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!--轨迹高度与按钮的宽度保持一致-->
    <size android:height="@dimen/ios_abc_thumb_width" />
    <!-- 圆角弧度 -->
    <corners android:radius="@dimen/ios_abc_switch_material" />

    <!-- 变化率  定义从左到右的颜色不变 -->
